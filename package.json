{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "npm": ">=8.0.0 <9.0.0",
    "node": ">=16.0.0 <17.0.0"
  },
  "dependencies": {
    "@apollo/client": "^3.7.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "@types/react-dom": "^18.0.10",
    "apollo": "^2.34.0",
    "axios": "^1.3.3",
    "dotenv": "^16.0.3",
    "graphql": "^16.6.0",
    "postcss-normalize": "^10.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "react-scripts": "^5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^3.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "CI=true react-scripts test --colors --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint './src/**/*.{ts,tsx}' --ext .ts,.tsx",
    "fix": "run-s fix:*",
    "lint:fix": "eslint './src/**/*.{ts,tsx}' --ext .tsx,.ts --fix",
    "fix:prettier": "prettier --write 'src/**/*.{ts,js,json}'",
    "fix:eslint": "eslint 'src/**/*.{js,ts,tsx}' --ext .js,.ts --fix"
  },
  "husky": {
    "hooks": {
      "pre-push": "lint-staged && yarn test"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "node-sass": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4"
  }
}
